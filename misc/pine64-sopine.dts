/dts-v1/;

/ {
	interrupt-parent = < 0x01 >;
	#address-cells = < 0x01 >;
	#size-cells = < 0x01 >;
	model = "SoPine with baseboard";
	compatible = "pine64,sopine-baseboard\0pine64,sopine\0allwinner,sun50i-a64";

	chosen {
		#address-cells = < 0x01 >;
		#size-cells = < 0x01 >;
		ranges;

		framebuffer-lcd {
			compatible = "allwinner,simple-framebuffer\0simple-framebuffer";
			allwinner,pipeline = "mixer0-lcd0";
			clocks = < 0x02 0x64 0x02 0x63 0x02 0x34 >;
			status = "disabled";
		};
	};

	opp_table0 {
		compatible = "operating-points-v2";
		opp-shared;
		phandle = < 0x03 >;

		opp-648000000 {
			opp-hz = < 0x00 0x269fb200 >;
			opp-microvolt = < 0xfde80 >;
			clock-latency-ns = < 0x3b9b0 >;
		};

		opp-816000000 {
			opp-hz = < 0x00 0x30a32c00 >;
			opp-microvolt = < 0x10c8e0 >;
			clock-latency-ns = < 0x3b9b0 >;
		};

		opp-912000000 {
			opp-hz = < 0x00 0x365c0400 >;
			opp-microvolt = < 0x111700 >;
			clock-latency-ns = < 0x3b9b0 >;
		};

		opp-960000000 {
			opp-hz = < 0x00 0x39387000 >;
			opp-microvolt = < 0x11b340 >;
			clock-latency-ns = < 0x3b9b0 >;
		};

		opp-1008000000 {
			opp-hz = < 0x00 0x3c14dc00 >;
			opp-microvolt = < 0x124f80 >;
			clock-latency-ns = < 0x3b9b0 >;
		};

		opp-1056000000 {
			opp-hz = < 0x00 0x3ef14800 >;
			opp-microvolt = < 0x12ebc0 >;
			clock-latency-ns = < 0x3b9b0 >;
		};

		opp-1104000000 {
			opp-hz = < 0x00 0x41cdb400 >;
			opp-microvolt = < 0x1339e0 >;
			clock-latency-ns = < 0x3b9b0 >;
		};

		opp-1152000000 {
			opp-hz = < 0x00 0x44aa2000 >;
			opp-microvolt = < 0x13d620 >;
			clock-latency-ns = < 0x3b9b0 >;
		};
	};

	cpus {
		#address-cells = < 0x01 >;
		#size-cells = < 0x00 >;

		cpu@0 {
			compatible = "arm,cortex-a53\0arm,armv8";
			device_type = "cpu";
			reg = < 0x00 >;
			enable-method = "psci";
			clocks = < 0x02 0x15 >;
			clock-names = "cpu";
			operating-points-v2 = < 0x03 >;
			#cooling-cells = < 0x02 >;
			cpu-supply = < 0x04 >;
			phandle = < 0x0f >;
		};

		cpu@1 {
			compatible = "arm,cortex-a53\0arm,armv8";
			device_type = "cpu";
			reg = < 0x01 >;
			enable-method = "psci";
			operating-points-v2 = < 0x03 >;
		};

		cpu@2 {
			compatible = "arm,cortex-a53\0arm,armv8";
			device_type = "cpu";
			reg = < 0x02 >;
			enable-method = "psci";
			operating-points-v2 = < 0x03 >;
		};

		cpu@3 {
			compatible = "arm,cortex-a53\0arm,armv8";
			device_type = "cpu";
			reg = < 0x03 >;
			enable-method = "psci";
			operating-points-v2 = < 0x03 >;
		};
	};

	display-engine {
		compatible = "allwinner,sun50i-a64-display-engine";
		allwinner,pipelines = < 0x05 0x06 >;
		status = "okay";
	};

	osc24M_clk {
		#clock-cells = < 0x00 >;
		compatible = "fixed-clock";
		clock-frequency = < 0x16e3600 >;
		clock-output-names = "osc24M";
		phandle = < 0x1f >;
	};

	osc32k_clk {
		#clock-cells = < 0x00 >;
		compatible = "fixed-clock";
		clock-frequency = < 0x8000 >;
		clock-output-names = "osc32k";
		phandle = < 0x20 >;
	};

	internal-osc-clk {
		#clock-cells = < 0x00 >;
		compatible = "fixed-clock";
		clock-frequency = < 0xf42400 >;
		clock-accuracy = < 0x11e1a300 >;
		clock-output-names = "iosc";
		phandle = < 0x31 >;
	};

	psci {
		compatible = "arm,psci-0.2";
		method = "smc";
	};

	sound_spdif {
		compatible = "simple-audio-card";
		simple-audio-card,name = "On-board SPDIF";

		simple-audio-card,cpu {
			sound-dai = < 0x07 >;
		};

		simple-audio-card,codec {
			sound-dai = < 0x08 >;
		};
	};

	spdif-out {
		#sound-dai-cells = < 0x00 >;
		compatible = "linux,spdif-dit";
		phandle = < 0x08 >;
	};

	reserved-memory {
		#address-cells = < 0x01 >;
		#size-cells = < 0x01 >;
		ranges;

		linux,cma {
			compatible = "shared-dma-pool";
			reusable;
			size = < 0x4000000 >;
			alignment = < 0x2000 >;
			linux,cma-default;
			phandle = < 0x2a >;
		};
	};

	sound {
		compatible = "simple-audio-card";
		simple-audio-card,name = "SUN50i Audio Card";
		simple-audio-card,format = "i2s";
		simple-audio-card,frame-master = < 0x09 >;
		simple-audio-card,bitclock-master = < 0x09 >;
		simple-audio-card,mclk-fs = < 0x200 >;
		simple-audio-card,aux-devs = < 0x0a >;
		simple-audio-card,routing = "Left DAC\0AIF1 Slot 0 Left\0Right DAC\0AIF1 Slot 0 Right";
		status = "okay";

		simple-audio-card,cpu {
			sound-dai = < 0x0b >;
			phandle = < 0x09 >;
		};

		simple-audio-card,codec {
			sound-dai = < 0x0c >;
		};
	};

	thermal-zones {

		cpu0-thermal {
			polling-delay-passive = < 0xfa >;
			polling-delay = < 0x3e8 >;
			thermal-sensors = < 0x0d 0x00 >;

			cooling-maps {

				map0 {
					trip = < 0x0e >;
					cooling-device = < 0x0f 0xffffffff 0xffffffff >;
				};

				map1 {
					trip = < 0x10 >;
					cooling-device = < 0x0f 0xffffffff 0xffffffff >;
				};
			};

			trips {

				cpu_alert0 {
					temperature = < 0x124f8 >;
					hysteresis = < 0x7d0 >;
					type = "passive";
					phandle = < 0x0e >;
				};

				cpu_alert1 {
					temperature = < 0x15f90 >;
					hysteresis = < 0x7d0 >;
					type = "hot";
					phandle = < 0x10 >;
				};

				cpu_crit {
					temperature = < 0x1adb0 >;
					hysteresis = < 0x7d0 >;
					type = "critical";
				};
			};
		};

		gpu0-thermal {
			polling-delay-passive = < 0xfa >;
			polling-delay = < 0x3e8 >;
			thermal-sensors = < 0x0d 0x01 >;
		};

		gpu1-thermal {
			polling-delay-passive = < 0xfa >;
			polling-delay = < 0x3e8 >;
			thermal-sensors = < 0x0d 0x02 >;
		};
	};

	timer {
		compatible = "arm,armv8-timer";
		allwinner,sun50i-a64-unstable-timer;
		interrupts = < 0x01 0x0d 0xf04 0x01 0x0e 0xf04 0x01 0x0b 0xf04 0x01 0x0a 0xf04 >;
	};

	soc {
		compatible = "simple-bus";
		#address-cells = < 0x01 >;
		#size-cells = < 0x01 >;
		ranges;

		clock@1000000 {
			compatible = "allwinner,sun50i-h5-de2-clk\0allwinner,sun50i-a64-de2-clk";
			reg = < 0x1000000 0x100000 >;
			clocks = < 0x02 0x63 0x02 0x34 >;
			clock-names = "mod\0bus";
			resets = < 0x02 0x1e >;
			#clock-cells = < 0x01 >;
			#reset-cells = < 0x01 >;
			phandle = < 0x11 >;
		};

		mixer@1100000 {
			compatible = "allwinner,sun50i-a64-de2-mixer-0";
			reg = < 0x1100000 0x100000 >;
			clocks = < 0x11 0x06 0x11 0x00 >;
			clock-names = "mod\0bus";
			resets = < 0x11 0x00 >;
			status = "disabled";
			phandle = < 0x05 >;

			ports {
				#address-cells = < 0x01 >;
				#size-cells = < 0x00 >;

				port@1 {
					#address-cells = < 0x01 >;
					#size-cells = < 0x00 >;
					reg = < 0x01 >;

					endpoint@0 {
						reg = < 0x00 >;
						remote-endpoint = < 0x12 >;
						phandle = < 0x14 >;
					};
				};
			};
		};

		mixer@1200000 {
			compatible = "allwinner,sun50i-a64-de2-mixer-1";
			reg = < 0x1200000 0x100000 >;
			clocks = < 0x11 0x01 0x11 0x07 >;
			clock-names = "bus\0mod";
			resets = < 0x11 0x01 >;
			phandle = < 0x06 >;

			ports {
				#address-cells = < 0x01 >;
				#size-cells = < 0x00 >;

				port@1 {
					reg = < 0x01 >;

					endpoint {
						remote-endpoint = < 0x13 >;
						phandle = < 0x15 >;
					};
				};
			};
		};

		syscon@1c00000 {
			compatible = "allwinner,sun50i-a64-system-controller\0syscon";
			reg = < 0x1c00000 0x1000 >;
			phandle = < 0x26 >;
		};

		dma-controller@1c02000 {
			compatible = "allwinner,sun50i-a64-dma";
			reg = < 0x1c02000 0x1000 >;
			interrupts = < 0x00 0x32 0x04 >;
			clocks = < 0x02 0x1e >;
			dma-channels = < 0x08 >;
			dma-requests = < 0x1b >;
			resets = < 0x02 0x07 >;
			#dma-cells = < 0x01 >;
			phandle = < 0x21 >;
		};

		lcd-controller@01c0c000 {
			compatible = "allwinner,sun50i-a64-tcon-lcd";
			reg = < 0x1c0c000 0x1000 >;
			interrupts = < 0x00 0x56 0x04 >;
			clocks = < 0x02 0x2f 0x02 0x64 >;
