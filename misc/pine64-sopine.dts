			reg = < 0x1c20800 0x400 >;
			interrupts = < 0x00 0x0b 0x04 0x00 0x11 0x04 0x00 0x15 0x04 >;
			clocks = < 0x02 0x3a >;
			gpio-controller;
			#gpio-cells = < 0x03 >;
			interrupt-controller;
			#interrupt-cells = < 0x03 >;

			i2c0_pins {
				pins = "PH0\0PH1";
				function = "i2c0";
			};

			i2c1_pins {
				pins = "PH2\0PH3";
				function = "i2c1";
			};

			lcd-rgb666 {
				pins = "PD0\0PD1\0PD2\0PD3\0PD4\0PD5\0PD6\0PD7\0PD8\0PD9\0PD10\0PD11\0PD12\0PD13\0PD14\0PD15\0PD16\0PD17\0PD18\0PD19\0PD20\0PD21";
				function = "lcd0";
			};

			mmc0-pins {
				pins = "PF0\0PF1\0PF2\0PF3\0PF4\0PF5";
				function = "mmc0";
				drive-strength = < 0x1e >;
				bias-pull-up;
				phandle = < 0x17 >;
			};

			mmc1-pins {
				pins = "PG0\0PG1\0PG2\0PG3\0PG4\0PG5";
				function = "mmc1";
				drive-strength = < 0x1e >;
				bias-pull-up;
				phandle = < 0x19 >;
