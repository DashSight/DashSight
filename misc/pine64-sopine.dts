		usb@1c19000 {
			compatible = "allwinner,sun8i-a33-musb";
			reg = < 0x1c19000 0x400 >;
			clocks = < 0x02 0x29 >;
			resets = < 0x02 0x12 >;
			interrupts = < 0x00 0x47 0x04 >;
