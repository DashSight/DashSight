			reg = < 0x1c10000 0x1000 >;
			clocks = < 0x02 0x20 0x02 0x4c >;
			clock-names = "ahb\0mmc";
			resets = < 0x02 0x09 >;
			reset-names = "ahb";
			interrupts = < 0x00 0x3d 0x04 >;
			max-frequency = < 0x8f0d180 >;
			status = "okay";
			#address-cells = < 0x01 >;
			#size-cells = < 0x00 >;
			pinctrl-names = "default";
			pinctrl-0 = < 0x19 >;
			vmmc-supply = < 0x1a >;
