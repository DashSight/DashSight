			reg = < 0x1c11000 0x1000 >;
			clocks = < 0x02 0x21 0x02 0x4d >;
			clock-names = "ahb\0mmc";
			resets = < 0x02 0x0a >;
			reset-names = "ahb";
			interrupts = < 0x00 0x3e 0x04 >;
			max-frequency = < 0xbebc200 >;
			status = "okay";
			#address-cells = < 0x01 >;
			#size-cells = < 0x00 >;
			pinctrl-names = "default";
			pinctrl-0 = < 0x1c >;
			vmmc-supply = < 0x18 >;
			vqmmc-supply = < 0x1d >;
			bus-width = < 0x08 >;
			non-removable;
			cap-mmc-hw-reset;
			mmc-hs200-1_8v;
