		usb@1c1b400 {
			compatible = "allwinner,sun50i-a64-ohci\0generic-ohci";
			reg = < 0x1c1b400 0x100 >;
			interrupts = < 0x00 0x4b 0x04 >;
			clocks = < 0x02 0x2d 0x02 0x5d >;
			resets = < 0x02 0x16 >;
			phys = < 0x1e 0x01 >;
			phy-names = "usb";
			status = "okay";
		};

		clock@1c20000 {
			compatible = "allwinner,sun50i-a64-ccu";
			reg = < 0x1c20000 0x400 >;
			clocks = < 0x1f 0x20 >;
			clock-names = "hosc\0losc";
			#clock-cells = < 0x01 >;
			#reset-cells = < 0x01 >;
			phandle = < 0x02 >;
		};

		pinctrl@1c20800 {
