		spdif@1c21000 {
			#sound-dai-cells = < 0x00 >;
			compatible = "allwinner,sun50i-a64-spdif\0allwinner,sun8i-h3-spdif";
			reg = < 0x1c21000 0x400 >;
			interrupts = < 0x00 0x0c 0x04 >;
